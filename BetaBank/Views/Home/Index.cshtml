                                <h2 class="title2">Your bank Name</h2>
                                <h5>Move money in easy secure way</h5>
                                <div class="layer-1-3">
                                    <a asp-page="/Register" class="ready-btn">Get started</a>
                                </div>
                            </div>
                            <!-- End slide content -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End intro Area -->
