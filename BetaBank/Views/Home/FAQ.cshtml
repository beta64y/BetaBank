@{
    ViewData["Title"] = "FAQ";
}

<!-- Start FAQ -->
<div class="faq-page bga area-padding mt-5">
    <div class="container ">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="section-headline text-center">
                    <h3 class="sds">Important FAQ</h3>
                    <p class="sds"> Below are some frequently asked questions.</p>
                </div>
            </div>
        </div>
        <div class="row">
            <!-- Start FAQ Start -->
            <div class="all-faq-text d-flex">
                <div class="col-lg-4 col-md-4 col-sm-12">
                    <div class="business-faq">
                        <h3>Beta Bank Frequently Asked Questions</h3>
                        <p>Find answers to common questions about our services, account management, and more. If you need further assistance, don't hesitate to contact our support team.</p>
                    </div>
                </div>
                <div class="col-lg-8 col-md-8 col-sm-12">
                    <div class="faq-full">
                        <div class="single-faq">
                            <h4>01. Account Management</h4>
                        </div>
                        <div class="faq-details">
                            <div class="panel-group" id="accordion">
                                <div class="accordion" id="accordionExample">
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingOne">
                                            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                                How do I open a new account?
                                            </button>
                                        </h2>
                                        <div id="collapseOne" class="accordion-collapse collapse show" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
                                            <div class="accordion-body">
                                                <strong>To open a new account, visit our website and click on the 'Open Account' button. Follow the on-screen instructions to complete the application process. You will need to provide personal details and identification documents.</strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingTwo">
                                            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                                How can I reset my password?
                                            </button>
                                        </h2>
                                        <div id="collapseTwo" class="accordion-collapse collapse" aria-labelledby="headingTwo" data-bs-parent="#accordionExample">
                                            <div class="accordion-body">
                                                <strong>If you need to reset your password, go to the login page and click on 'Forgot Password'. Follow the instructions to reset your password via the email address associated with your account.</strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingThree">
                                            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                                How do I update my personal information?
                                            </button>
                                        </h2>
                                        <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree" data-bs-parent="#accordionExample">
                                            <div class="accordion-body">
                                                <strong>To update your personal information, log in to your account and navigate to the 'Profile' section. Here, you can edit your contact details, address, and other personal information. Make sure to save your changes before exiting.</strong>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <!-- Start FAQ Start -->
            <div class="all-faq-text d-flex">
                <div class="col-lg-4 col-md-4 col-sm-12">
                    <div class="business-faq">
                        <h3>Online Banking FAQ questions.</h3>
                        <p>Explore answers to your most pressing questions about our online banking services. Get detailed information on accessing, managing, and troubleshooting your online account.</p>
                    </div>

                </div>
                <div class="col-lg-8 col-md-8 col-sm-12">
                    <div class="faq-full">
                        <div class="single-faq">
                            <h4>02. Online Banking</h4>
                        </div>
                        <div class="faq-details">
                            <div class="panel-group" id="accordion">
                                <div class="accordion" id="accordionExampleTwo">
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingFour">
                                            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseFour" aria-expanded="true" aria-controls="collapseFour">
                                                How do I access online banking?
                                            </button>
                                        </h2>
                                        <div id="collapseFour" class="accordion-collapse collapse show" aria-labelledby="headingFour" data-bs-parent="#accordionExampleTwo">
                                            <div class="accordion-body">
                                                <strong>To access online banking, visit our website and click on the 'Login' button at the top right corner. Enter your username and password to access your account. If you haven't registered for online banking, follow the prompts to create an account.</strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingFive">
                                            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
                                                What should I do if I encounter technical issues?
                                            </button>
                                        </h2>
                                        <div id="collapseFive" class="accordion-collapse collapse" aria-labelledby="headingFive" data-bs-parent="#accordionExampleTwo">
                                            <div class="accordion-body">
                                                <strong>If you experience technical issues with online banking, first try clearing your browser's cache and cookies. If the problem persists, contact our technical support team through the contact form on our website for further assistance.</strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingSix">
                                            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseSix" aria-expanded="false" aria-controls="collapseSix">
                                                Can I view my transaction history online?
                                            </button>
                                        </h2>
                                        <div id="collapseSix" class="accordion-collapse collapse" aria-labelledby="headingSix" data-bs-parent="#accordionExampleTwo">
                                            <div class="accordion-body">
                                                <strong>Yes, you can view your transaction history by logging into your online banking account and navigating to the 'Account Statements' or 'Transaction History' section. You can filter and download statements as needed.</strong>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <!-- Start FAQ Start -->
            <div class="all-faq-text d-flex">
                <div class="col-lg-4 col-md-4 col-sm-12">
                    <div class="business-faq">
                        <h3>Loan Services FAQ</h3>
                        <p>Discover everything you need to know about our loan offerings, from application processes to repayment options. Our detailed FAQs are here to guide you through every step of your loan journey.</p>
                    </div>
                </div>
                <div class="col-lg-8 col-md-8 col-sm-12">
                    <div class="faq-full">
                        <div class="single-faq">
                            <h4>03. Loan Services</h4>
                        </div>
                        <div class="faq-details">
                            <div class="panel-group" id="accordion">
                                <div class="accordion" id="accordionExampleThree">
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingSeven">
                                            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseSeven" aria-expanded="true" aria-controls="collapseSeven">
                                                What types of loans does Bet Bank offer?
                                            </button>
                                        </h2>
                                        <div id="collapseSeven" class="accordion-collapse collapse show" aria-labelledby="headingSeven" data-bs-parent="#accordionExampleThree">
                                            <div class="accordion-body">
                                                <strong>Bet Bank offers a variety of loan products, including personal loans, home loans, auto loans, and business loans. Each type of loan has specific terms and conditions, so please visit our website or contact our loan department for more details.</strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingEight">
                                            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseEight" aria-expanded="false" aria-controls="collapseEight">
                                                How do I apply for a loan?
                                            </button>
                                        </h2>
                                        <div id="collapseEight" class="accordion-collapse collapse" aria-labelledby="headingEight" data-bs-parent="#accordionExampleThree">
                                            <div class="accordion-body">
                                                <strong>To apply for a loan, visit our website and fill out the online application form. You will need to provide personal information, financial details, and any relevant documents. Alternatively, you can visit one of our branches to apply in person.</strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="headingNine">
                                            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseNine" aria-expanded="false" aria-controls="collapseNine">
                                                What are the eligibility criteria for loans?
                                            </button>
                                        </h2>
                                        <div id="collapseNine" class="accordion-collapse collapse" aria-labelledby="headingNine" data-bs-parent="#accordionExampleThree">
                                            <div class="accordion-body">
                                                <strong>Eligibility criteria vary depending on the type of loan. Generally, you need to be at least 18 years old, have a stable income, and meet our credit requirements. Specific criteria for each loan type are detailed on our website or can be provided by our loan advisors.</strong>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End FAQ -->


@section Styles {
    <link rel="stylesheet" href="~/form.css" asp-append-version="true">

}
@await Component.InvokeAsync("Support")
@section Scripts {
   <script src="~/lib/bootstrap-5.3.3-dist/js/bootstrap.bundle.js"></script>




}
