    <![endif]-->

    <div id="preloader"></div>
    <!-- Start Header Area -->
    @await Component.InvokeAsync("Header")

    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    @await Component.InvokeAsync("Footer")
    <!-- Start footer area -->
    

    @*<footer class="border-top footer text-muted">
    <div class="container">
    &copy; 2022 - Bank - <a  asp-page="/Privacy">Privacy</a>
    </div>
    </footer>*@

