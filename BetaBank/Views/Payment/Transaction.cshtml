@{
    ViewData["Title"] = "Transaction";
    var data = ViewData["Transaction"] as Transaction;
    Layout = null;
}
<div class="transaction-details">
    <div class="form-group">
        <label>Transaction ID:</label>
        <p>@data.Id</p>
    </div>
    <div class="form-group">
        <label>Receipt Number:</label>
        <p>@data.ReceiptNumber</p>
    </div>
    <div class="form-group">
        <label>Amount:</label>
        <p>@data.Amount.ToString("C")</p>
    </div>
    <div class="form-group">
        <label>Commission:</label>
        <p>@data.Commission.ToString("C")</p>
    </div>
    <div class="form-group">
        <label>Billing Amount:</label>
        <p>@data.BillingAmount.ToString("C")</p>
    </div>
    <div class="form-group">
        <label>Cashback Amount:</label>
        <p>@data.CashbackAmount.ToString("C")</p>
    </div>
    <div class="form-group">
        <label>Transaction Date:</label>
        <p>@data.TransactionDate.ToString("f")</p>
    </div>
    <div class="form-group">
        <label>Paid By Type:</label>
        <p>@data.PaidByTypeId</p>
    </div>
    <div class="form-group">
        <label>Paid By ID:</label>
        <p>@data.PaidById</p>
    </div>
    <div class="form-group">
        <label>Destination Type:</label>
        <p>@data.DestinationTypeId</p>
    </div>
    <div class="form-group">
        <label>Destination ID:</label>
        <p>@data.DestinationId</p>
    </div>
    <div class="form-group">
        <label>Status ID:</label>
        <p>@data.StatusId</p>
    </div>
    <div class="form-group">
        <label>Icon:</label>
        <p>@data.Icon</p>
    </div>
</div>